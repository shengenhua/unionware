<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_packing_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_1"
        android:gravity="center"
        android:paddingVertical="@dimen/dp_2"
        android:text="包装条码"
        android:textSize="@dimen/sp_16"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_content"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_packing_capacity_title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginHorizontal="@dimen/dp_1"
        android:gravity="center"
        android:text="箱容量"
        android:textSize="@dimen/sp_16"
        app:layout_constraintBottom_toBottomOf="@+id/tv_packing_title"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_capacity_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_capacity_content"
        app:layout_constraintTop_toTopOf="@id/tv_packing_title" />


    <TextView
        android:id="@+id/tv_detalis_md_title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/dp_1"
        android:gravity="center"
        android:text="已装件数"
        android:textSize="@dimen/sp_16"
        app:layout_constraintBottom_toBottomOf="@+id/tv_packing_title"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_num_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_num_content"
        app:layout_constraintTop_toTopOf="@id/tv_packing_title" />

    <TextView
        android:id="@+id/tv_packing_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/dp_2"
        android:paddingHorizontal="@dimen/dp_4"
        android:paddingVertical="@dimen/dp_5"
        android:text="暂无数据"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@id/tv_packing_capacity_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_packing_title" />

    <TextView
        android:id="@+id/tv_packing_capacity_content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="暂无数据"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/tv_packing_content"
        app:layout_constraintEnd_toStartOf="@+id/tv_packing_num_content"
        app:layout_constraintStart_toEndOf="@+id/tv_packing_content"
        app:layout_constraintTop_toTopOf="@+id/tv_packing_content" />

    <TextView
        android:id="@+id/tv_packing_num_content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/dp_2"
        android:text="暂无数据"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/tv_packing_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_packing_capacity_content"
        app:layout_constraintTop_toTopOf="@+id/tv_packing_content" />


    <TextView
        android:id="@+id/tv_packing_sup_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_1"
        android:gravity="center"
        android:paddingVertical="@dimen/dp_2"
        android:text="供应商"
        android:textSize="@dimen/sp_16"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_content"
        app:layout_constraintTop_toBottomOf="@+id/tv_packing_content" />

    <TextView
        android:id="@+id/tv_packing_sup_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_1"
        android:gravity="center"
        android:paddingVertical="@dimen/dp_5"
        android:text="暂无数据"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_content"
        app:layout_constraintTop_toBottomOf="@+id/tv_packing_sup_title" />

    <TextView
        android:id="@+id/tv_packing_exp_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_1"
        android:gravity="center"
        android:paddingVertical="@dimen/dp_2"
        android:text="快递单号"
        android:textSize="@dimen/sp_16"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_capacity_content"
        app:layout_constraintStart_toStartOf="@+id/tv_packing_capacity_content"
        app:layout_constraintTop_toBottomOf="@+id/tv_packing_capacity_content" />

    <TextView
        android:id="@+id/tv_packing_exp_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/dp_2"
        android:paddingHorizontal="@dimen/dp_4"
        android:paddingVertical="@dimen/dp_5"
        android:text="暂无数据"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tv_packing_exp_title"
        app:layout_constraintStart_toStartOf="@id/tv_packing_exp_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_packing_exp_title" />

</androidx.constraintlayout.widget.ConstraintLayout>